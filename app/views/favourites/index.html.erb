
<table>
  <% for favourite in @favourites %>
  <tr>
    <td>
       <%= link_to favourite.post.title, post_path(favourite.post)%>
    </td>
    <td>
      By : <%= favourite.post.user.get_full_name %>
    </td>
  </tr>
  <% end %>
</table>
<tr>
  <td class="pages">
      <%= paginate @favourites %>
  </td>
</tr>
